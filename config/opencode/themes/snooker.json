{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "bg": "#0f171b",
    "bg2": "#141f1f",
    "fg": "#adad9b",
    "bg_light": "#2b302b",
    "red": "#2E3440",
    "green": "#1c9c40",
    "brown": "#a57230",
    "blue": "#0094cf",
    "purple": "#8b72da",
    "cyan": "#1dae87",
    "fg_light": "#cdc08b",
    "fg_com": "#6a6a5b",
    "orange": "#ea901a",
    "green_light": "#22bd4e",
    "yellow": "#c89b13",
    "blue_light": "#00a3cc",
    "pink": "#df7376",
    "cyan_light": "#4abe65",
    "fg_bright": "#e5e5d2"
  },
  "theme": {
    "primary": {
      "dark": "fg_com",
      "light": "fg_com"
    },
    "secondary": {
      "dark": "cyan",
      "light": "cyan"
    },
    "accent": {
      "dark": "purple",
      "light": "purple"
    },
    "error": {
      "dark": "red",
      "light": "red"
    },
    "warning": {
      "dark": "orange",
      "light": "orange"
    },
    "success": {
      "dark": "green_light",
      "light": "green_light"
    },
    "info": {
      "dark": "purple",
      "light": "purple"
    },
    "text": {
      "dark": "fg",
      "light": "bg_light"
    },
    "textMuted": {
      "dark": "fg_com",
      "light": "fg_com"
    },
    "background": {
      "dark": "bg",
      "light": "fg_bright"
    },
    "backgroundPanel": {
      "dark": "bg2",
      "light": "fg_com"
    },
    "backgroundElement": {
      "dark": "bg_light",
      "light": "bg_light"
    },
    "border": {
      "dark": "fg",
      "light": "fg"
    },
    "borderActive": {
      "dark": "blue",
      "light": "blue"
    },
    "borderSubtle": {
      "dark": "fg_com",
      "light": "fg_com"
    },
    "diffAdded": {
      "dark": "green_light",
      "light": "green_light"
