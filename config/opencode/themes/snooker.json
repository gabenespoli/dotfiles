    "diffRemovedLineNumberBg": {
      "dark": "#301a1f",
      "light": "#301a1f"
    },
    "markdownText": {
      "dark": "fg",
      "light": "bg_light"
    },
    "markdownHeading": {
      "dark": "fg_bright",
      "light": "bg_light"
    },
    "markdownLink": {
      "dark": "brown",
      "light": "brown"
    },
    "markdownLinkText": {
      "dark": "blue",
      "light": "blue"
    },
    "markdownCode": {
      "dark": "cyan_light",
      "light": "cyan_light"
    },
    "markdownBlockQuote": {
      "dark": "brown",
      "light": "brown"
    },
    "markdownEmph": {
      "dark": "blue_light",
      "light": "blue_light"
    },
    "markdownStrong": {
      "dark": "fg_light",
      "light": "fg_light"
    },
    "markdownHorizontalRule": {
      "dark": "fg_com",
      "light": "fg_com"
    },
    "markdownListItem": {
      "dark": "fg_com",
      "light": "fg_com"
    },
    "markdownListEnumeration": {
      "dark": "fg_light",
      "light": "fg_light"
    },
    "markdownImage": {
      "dark": "orange",
      "light": "orange"
    },
    "markdownImageText": {
      "dark": "fg_light",
      "light": "fg_light"
    },
    "markdownCodeBlock": {
      "dark": "fg",
      "light": "fg"
    },
    "syntaxComment": {
      "dark": "fg_com",
      "light": "fg_com"
    },
    "syntaxKeyword": {
      "dark": "green",
      "light": "green"
    },
    "syntaxFunction": {
      "dark": "blue",
      "light": "blue"
    },
    "syntaxVariable": {
      "dark": "fg_light",
      "light": "fg_light"
    },
    "syntaxString": {
      "dark": "cyan",
      "light": "cyan"
    },
    "syntaxNumber": {
      "dark": "cyan",
      "light": "cyan"
    },
    "syntaxType": {
      "dark": "yellow",
      "light": "yellow"
    },
    "syntaxOperator": {
      "dark": "fg",
      "light": "fg"
    },
    "syntaxPunctuation": {
      "dark": "fg",
      "light": "fg"
    }
  }
