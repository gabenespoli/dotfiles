    "diffRemoved": {
      "dark": "red",
      "light": "red"
    },
    "diffContext": {
      "dark": "brown",
      "light": "brown"
    },
    "diffHunkHeader": {
      "dark": "purple",
      "light": "purple"
    },
    "diffHighlightAdded": {
      "dark": "green_light",
      "light": "green_light"
    },
    "diffHighlightRemoved": {
      "dark": "red",
      "light": "red"
    },
    "diffAddedBg": {
      "dark": "#12261e",
      "light": "#12261e"
    },
    "diffRemovedBg": {
      "dark": "#301a1f",
      "light": "#301a1f"
    },
    "diffContextBg": {
      "dark": "bg_light",
      "light": "fg_com"
    },
    "diffLineNumber": {
      "dark": "blue",
      "light": "blue"
