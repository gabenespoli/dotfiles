# unbind the tmuxnaviage keys, like hitting esc from within "tmuxnavigate mode"
# https://github.com/christoomey/vim-tmux-navigator/issues/59
unbind -n h
unbind -n j
unbind -n k
unbind -n l
unbind -n w
unbind -n \;

unbind -n C-h
unbind -n C-j
unbind -n C-k
unbind -n C-l
unbind -n C-w
unbind -n C-\;

bind -n C-w source-file ~/.tmux/tmuxnavigate_mode
