[alias]
  st = !"git-st"
  br = "branch"
  brm = "branch -m"
  co = "checkout"
  cob = "checkout -b"
  cp = "cherry-pick"
  rb = "rebase"
  ri = "rebase --interactive"
	ctags = !.git/hooks/ctags
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
[color.decorate]
  branch = 2
  remoteBranch = 4
  tag = 5
  stash = 6
  HEAD = 3
[init]
	templatedir = ~/.git_template
