# Use the command 'task show' to see all defaults and overrides

# Files
data.location=~/.task

# Color theme
include ~/.task/colors.theme

# TrelloWarrior
uda.trelloid.type=string
uda.trelloid.label=Trello ID
uda.trellolistname.type=string
uda.trellolistname.label=Trello List Name
uda.trellolistname.default=Todo

# Custom reports
report.t.description=My custom report
report.t.columns=id,depends,project,description,due.remaining,urgency
report.t.labels=ID,Deps,Proj,Description,Due,Urg
report.t.sort=urgency-/,project+
report.t.filter=status:pending and (trellolistname:Todo or trellolistname:Doing)
